<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:padding="20dp"
        tools:context=".operator.TransformOperatorExampleActivity">

        <Button
            android:id="@+id/bt_take"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Take Example" />

        <Button
            android:id="@+id/bt_take_timer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Timer Take Example" />

        <Button
            android:id="@+id/bt_skip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Skip Example" />

        <Button
            android:id="@+id/bt_filter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Filter Example" />

        <Button
            android:id="@+id/bt_map"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Map Example" />

        <Button
            android:id="@+id/bt_flatmap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Flatmap Example" />

        <Button
            android:id="@+id/bt_switchmap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Switchmap Example" />

        <Button
            android:id="@+id/bt_concatmap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Concatmap Example" />
    </LinearLayout>
</layout>